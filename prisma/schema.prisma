
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  nome  String
  endereco String?
  senha String
  Announcement Announcement[]
  @@map("usuario")
}
model Announcement {
  id Int        @id @default(autoincrement())
  anunciante User @relation(fields: [anuncianteId], references: [id])
  anuncianteId Int
  veiculo String
  placa String 
  preco Float
  linkSocial String
  @@map("anuncio")
}
