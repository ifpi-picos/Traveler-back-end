
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  nome  String
  endereco String?
  senha String
  Announcement Announcement[]
}
model Announcement {
  id Int        @id @default(autoincrement())
  nomeAnunciante String
  veiculo String
  placa String 
  preco Float
  linkSocial String
  User User[]
  
}